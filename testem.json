{
  "test_page": "test/index.html",
  "parallel": 5,
  "framework": "mocha",
  "before_tests": "npm run build",
  "launchers": {
    "Mocha": {
      "command": "./node_modules/.bin/mocha --reporter=spec dist/test/browserify.js"
    },
    "SL_chrome": {
      "exe": "./node_modules/.bin/ember-cli-sauce",
      "args": [
        "-b",
        "chrome",
        "--at",
        "--no-ct",
        "--u"
      ],
      "protocol": "browser"
    }
  },
  "launch_in_ci": [
    "SL_chrome",
    "mocha"
  ],
  "launch_in_dev": [
    "SL_chrome",
    "Safari",
    "mocha"
  ]
}
